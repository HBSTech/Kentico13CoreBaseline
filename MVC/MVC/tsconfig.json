{
    "compilerOptions": {
      "noImplicitAny": false,
      "noEmitOnError": true,
      "removeComments": false,
      "sourceMap": true,
      "module": "commonjs",
      "target": "es5",
      "jsx": "react",
      "strict": true,
      "lib": [
        "es2015",
        "es2015.promise",
        "es2015.iterable",
        "dom"
      ]
    },
    "include": [ "FrontEndDev/typescript/**/*",
                  "FrontEndDev/react/**/*" ],
    "compileOnSave": true,
    "exclude": [
      "node_modules",
      "wwwroot"
    ]
} 