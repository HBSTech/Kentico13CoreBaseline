@model Navigation.Components.Navigation.NavigationViewModel
<ul class="navbar-nav @Model.NavWrapperClass" data-ktc-search-exclude>
    @foreach (var NavItem in Model.NavItems)
    {
        <partial name="/Components/Navigation/MainNavigation/MainNavigationItem.cshtml" for="@NavItem" />
    }
</ul>
@{
    if(Model.IncludeScreenReaderNavigation){
        <vc:screen-reader-navigation navigation-items="Model.NavItems" navigation-id="Model.NavWrapperClass" />
    }
}
